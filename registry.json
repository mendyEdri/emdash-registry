{
  "version": "0.1.0",
  "repository": "https://github.com/mendyEdri/emdash.dev",
  "base_path": "packages/registry",
  "skills": {
    "frontend-design": {
      "path": "skills/frontend-design/SKILL.md",
      "description": "Create distinctive, production-grade frontend interfaces with high design quality",
      "tags": ["frontend", "design", "ui", "css"],
      "user_invocable": true
    }
  },
  "rules": {},
  "agents": {
    "architect": {
      "path": "agents/architect.md",
      "description": "System design and architecture planning agent",
      "tags": ["architecture", "design", "planning"],
      "model": "sonnet-4-5"
    },
    "ui-browser-tester": {
      "path": "agents/ui-browser-tester.md",
      "description": "UI testing specialist that interacts with web applications through Playwright to verify functionality",
      "tags": ["testing", "ui", "browser", "playwright", "qa"],
      "model": "accounts/fireworks/models/minimax-m2p1"
    }
  },
  "verifiers": {
    "eslint": {
      "path": "verifiers/eslint.json",
      "description": "ESLint JavaScript/TypeScript linting",
      "tags": ["javascript", "typescript", "linting"],
      "type": "command"
    },
    "pytest": {
      "path": "verifiers/pytest.json",
      "description": "Python test runner with pytest",
      "tags": ["python", "testing"],
      "type": "command"
    },
    "type-check": {
      "path": "verifiers/type-check.json",
      "description": "TypeScript type checking",
      "tags": ["typescript", "types"],
      "type": "command"
    }
  }
}
